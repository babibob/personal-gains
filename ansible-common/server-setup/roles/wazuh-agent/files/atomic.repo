# Name: Atomic Rocket Turtle RPM Repository for Rocky / CentOS / Red Hat Enterprise Linux 8 - 
# URL: http://www.atomicrocketturtle.com/
[atomic]
name = Rocky / CentOS / Red Hat Enterprise Linux $releasever - atomic
mirrorlist = https://updates.atomicorp.com/channels/mirrorlist/atomic/centos-$releasever-$basearch
enabled = 1
protect = 0
gpgkey = https://www.atomicorp.com/RPM-GPG-KEY.atomicorp.txt
gpgcheck = 1

# Almost Stable, release candidates for [atomic]
[atomic-testing]
name = Rocky / CentOS / Red Hat Enterprise Linux $releasever - atomic - (Testing)
mirrorlist = https://updates.atomicorp.com/channels/mirrorlist/atomic-testing/centos-$releasever-$basearch
enabled = 0
protect = 0
gpgkey = https://www.atomicorp.com/RPM-GPG-KEY.atomicorp.txt
gpgcheck = 1
